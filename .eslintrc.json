{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint-config-prettier",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "eslint-plugin-prettier",
    "@typescript-eslint",
    "import",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error"],
    "no-param-reassign": ["error", { "props": false }]
  }
}
